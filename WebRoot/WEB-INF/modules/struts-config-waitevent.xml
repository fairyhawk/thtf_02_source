<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<data-sources />
	<form-beans>
	</form-beans>
	<global-exceptions />
	<global-forwards />
	<action-mappings> 
		<action path="/toworkcount"
			type="cn.com.thtf.egov.cms.action.work.TodoWorkAction" scope="request"
			validate="false" parameter="getTodoWorkCount">
		</action>
		<action path="/todolist"
			type="cn.com.thtf.egov.cms.action.work.TodoWorkAction" scope="request"
			validate="false" parameter="getTodoWorkList">
		</action>
	</action-mappings>
	<message-resources parameter="cn.com.tsinghua.mp.web.conf.i18n.ApplicationResources" />
</struts-config>
